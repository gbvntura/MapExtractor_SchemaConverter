<metadata xml:lang="en"><Esri><CreaDate>20191015</CreaDate><CreaTime>13592600</CreaTime><ArcGISFormat>1.0</ArcGISFormat><ArcGISstyle>FGDC CSDGM Metadata</ArcGISstyle><SyncOnce>TRUE</SyncOnce><ModDate>20210215</ModDate><ModTime>15325500</ModTime><scaleRange><minScale>150000000</minScale><maxScale>5000</maxScale></scaleRange><ArcGISProfile>FGDC</ArcGISProfile></Esri><dataIdInfo><idCitation><resTitle>AlacarteOrientPtsToGeMS</resTitle></idCitation></dataIdInfo><distInfo><distributor><distorFormat><formatName>ArcToolbox Tool</formatName></distorFormat></distributor></distInfo><tool name="alacarteOrientPtsToGeMS" displayname="AlacarteOrientPtsToGeMS" toolboxalias="SchemaConvert" xmlns=""><parameters><param name="fds" displayname="Existing GeMS Geologic Map feature dataset:" type="Required" direction="Input" datatype="Feature Dataset" expression="fds"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;An existing GeMS geologic map database where the Orientation Points feature classes will be added. &lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="points" displayname="Points features:" type="Required" direction="Input" datatype="Feature Class" expression="points"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The original Orientation Points data in Alacarte format. "PTTYPE", strike, dip, and label fields must be present regardless of their name. &lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="PTYPEname" displayname="Custom PTTYPE field name in original orientation points:" type="Optional" direction="Input" datatype="String" expression="{PTYPEname}"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;A "PTTYPE" field name other than "PTTYPE" (e.g. PTTYPE100k).&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="AzName" displayname="Custom STRIKE field name in original orientation points:" type="Optional" direction="Input" datatype="String" expression="{AzName}"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;A "STRIKE" field name other than "STRIKE".&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="IncName" displayname="Custom DIP field name in original orientation points:" type="Optional" direction="Input" datatype="String" expression="{IncName}"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;A "DIP" field name other than "DIP".&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="LabelName" displayname="Custom LABEL field name in original orientation points:" type="Optional" direction="Input" datatype="String" expression="{LabelName}"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;A "LABEL" field name other than "LABEL".&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="GEMS toolbox" displayname="GEMS toolbox:" type="Required" direction="Input" datatype="Toolbox" expression="GEMS toolbox"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;A path to the GeMS toolbox. Tested with the version on Ryan Crow's github site.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="crosswalk table" displayname="Crosswalk Table:" type="Optional" direction="Input" datatype="Text File" expression="{crosswalk table}"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;A "crosswalk" table or .txt. file for use in the GeMS's "Attribute By Key Values" tool.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="datasource" displayname="DataSourceID for this map:" type="Optional" direction="Input" datatype="String" expression="{datasource}"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;A DataSourceID to be used in the new GeMS database for all features. &lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="workspace" displayname="Temp Workspace:" type="Required" direction="Input" datatype="Workspace" expression="workspace"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;A folder where temporary files will be created. &lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param></parameters></tool><mdHrLv><ScopeCd value="005"/></mdHrLv></metadata>
